# Public URL of your PayloadCMS instance
NEXT_PUBLIC_CMS_URL=http://localhost:3001

# Secret for Next.js preview mode (/api/preview)
PREVIEW_SECRET=dev-preview-secret

# Secret for /api/revalidate webhook verification
NEXT_REVALIDATE_SECRET=dev-secret

# Optional: custom S3 host for Next Image (e.g., your-bucket.s3.amazonaws.com or CDN domain)
# Leave empty if using default s3.amazonaws.com
NEXT_PUBLIC_S3_HOST=
